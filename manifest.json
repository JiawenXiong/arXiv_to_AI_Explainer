{
  "manifest_version": 3,
  "default_locale": "zh_CN",
  "name": "__MSG_extensionName__",
  "version": "0.1",
  "author": {
    "email": "xiong.jia.wen@163.com"
  },
  "icons": {
    "128": "icons/icon.png"
  },
  "description": "__MSG_extensionDescription__",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://arxiv.org/abs/*","https://www.arxiv.org/abs/*"],
      "js": ["content_arxiv.js"]
    },
    {
      "matches": ["https://chatgpt.com/*"],
      "js": ["content_chatgpt.js"]
    },
    {
      "matches": ["https://gemini.google.com/*"],
      "js": ["content_gemini.js"]
    },
    {
      "matches": ["https://kimi.moonshot.cn/*", "https://www.kimi.com/*"],
      "js": ["content_kimi.js"]
    },
    {
      "matches": ["https://www.doubao.com/*"],
      "js": ["content_doubao.js"]
    }
  ]
}